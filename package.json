{
  "name": "vite-ssr-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "esno server",
    "build": "run-p build:client build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && npm run build:server && esno prerender",
    "serve": "cross-env NODE_ENV=production esno server",
    "serve:static": "serve dist/static"
  },
  "dependencies": {
    "@vueuse/head": "^0.2.3",
    "vite-pages": "^0.1.0",
    "vue": "^3.0.5",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@iconify/json": "^1.1.303",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.28",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.5",
    "autoprefixer": "^10.2.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.20.0",
    "esno": "^0.4.3",
    "express": "^4.17.1",
    "fast-glob": "^3.2.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-nested": "^5.0.3",
    "serve": "^11.3.2",
    "serve-static": "^1.14.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5",
    "vite": "^2.0.0",
    "vite-plugin-components": "^0.6.12",
    "vite-plugin-icons": "^0.2.2"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "ignorePatterns": [
      "dist/*",
      "**/vendor/*.js"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
